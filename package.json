{
  "name": "@nekw/shaderr-root",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@10.30.1",
  "description": "Shaders!",
  "author": {
    "name": "Neko Ayaka",
    "email": "neko@ayaka.moe",
    "url": "https://github.com/nekomeowww"
  },
  "license": "MIT",
  "homepage": "https://github.com/nekomeowww/shaderr",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nekomeowww/shaderr.git"
  },
  "bugs": "https://github.com/nekomeowww/shaderr/issues",
  "scripts": {
    "dev": "pnpm -rF=\"./packages/*\" -F=\"./examples/*\" -F=\"./apps/*\" run --parallel dev",
    "build": "pnpm -rF=\"./packages/*\" -F=\"./examples/*\" -F=\"./apps/*\" run build",
    "test": "vitest --coverage",
    "test:run": "vitest run",
    "lint": "eslint --cache .",
    "lint:fix": "eslint --cache --fix .",
    "typecheck": "pnpm -rF=\"./packages/*\" -F=\"./apps/*\" -F run --parallel typecheck",
    "up": "taze -w -r -I -f && pnpm prune && pnpm dedupe",
    "changelogithub": "changelogithub",
    "sizecheck": "npx vite-bundle-visualizer",
    "nolyfill": "pnpm dlx nolyfill"
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:",
    "@antfu/ni": "catalog:",
    "@antfu/nip": "catalog:",
    "@arethetypeswrong/cli": "catalog:",
    "@catppuccin/palette": "catalog:",
    "@pnpm/find-workspace-dir": "catalog:",
    "@types/node": "catalog:",
    "@unocss/eslint-config": "catalog:",
    "@unocss/eslint-plugin": "catalog:",
    "@unocss/preset-mini": "catalog:",
    "bumpp": "catalog:",
    "changelogithub": "catalog:",
    "eslint": "catalog:",
    "eslint-plugin-import-x": "catalog:",
    "rollup": "catalog:",
    "taze": "catalog:",
    "tsx": "catalog:",
    "typescript": "catalog:",
    "unbuild": "catalog:",
    "unocss": "catalog:",
    "unocss-preset-scrollbar": "catalog:",
    "vite": "catalog:",
    "vite-plugin-inspect": "catalog:",
    "vitest": "catalog:"
  }
}
